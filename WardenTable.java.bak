import javax.swing.*;  
public class WardenTable 
{  
    JFrame f;  
	WardenTable()
	{  
		f=new JFrame();   
		String column[]={"UNIQUE ID","FULL NAME","RANK,"};  
        JTable jt=new JTable(column);  
		jt.setBounds(10,10,1000,700);  
		JScrollPane sp=new JScrollPane(jt);  
		f.add(sp);  
		f.setBounds(50,50, 1200,600); 
		f.setVisible(true);  
	}
	public static void main(String[] args) {  
    new WardenTable();  
	}  
}  
